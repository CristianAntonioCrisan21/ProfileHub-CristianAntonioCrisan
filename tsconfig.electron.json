  "compilerOptions": {
    "noEmit": false,
    "outDir": "electron/dist",
    "rootDir": "electron",
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["electron/**/*.ts"]
}

