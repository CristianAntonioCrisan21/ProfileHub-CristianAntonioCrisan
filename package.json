{
  "name": "workprofiles-next",
  "version": "0.1.0",
  "private": true,
  "main": "electron/dist/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",

    "build:electron": "tsc -p tsconfig.electron.json",
    "watch:electron": "tsc -w -p tsconfig.electron.json",
    "dev:electron": "concurrently -k -p \"[{name}]\" -n NEXT,TS,ELEC -c auto \"next dev --turbopack\" \"tsc -w -p tsconfig.electron.json\" \"wait-on http-get://localhost:3000 file:electron/dist/main.js && electron electron/dist/main.js\"",
    "electron:dev": "wait-on http-get://localhost:3000 file:electron/dist/main.js && electron electron/dist/main.js"
  },



  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/electron": "^1.4.38",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "electron": "^38.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^8.0.4"
  }
}
